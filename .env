# Generate sekali, jangan pernah ganti setelah production
# Jalankan: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
MASTER_SECRET=paste_hasil_generate_disini

# Untuk HMAC signing
# Jalankan: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
HMAC_SECRET=paste_hasil_generate_disini

# Untuk admin access
# Jalankan: node -e "console.log(require('crypto').randomBytes(48).toString('hex'))"
ADMIN_SECRET=paste_hasil_generate_disini

# Mode
NODE_ENV=production

# Port (Railway auto-set, jangan ubah)
PORT=3000

# IP Anda (opsional, untuk whitelist)
OWNER_IP=

# Script refresh interval dalam menit
SCRIPT_CACHE_MINUTES=10

# Work.ink cache dalam menit
WORKINK_CACHE_MINUTES=5
